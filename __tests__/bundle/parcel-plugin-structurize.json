{
    "verbose": true,
    "displayAssetsMap": true,
    "rules": [
        {
            "match": "*.js",
            "folder": "js"
        },
        {
            "match": "*.css",
            "folder": "css"
        },
        {
            "match": "*.{jpg,jpeg,jpeg2,png,gif,svg,bmp,webp}",
            "folder": "assets"
        },
        {
            "match": "index.html",
            "folder": "."
        },
        {
            "match": "**/*.html",
            "folder": "hehe",
            "flatten": true
        }
    ]
}
